<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Theme">
        <!-- Navigation bar divider color, used by NavigationBarContentFrameLayout -->
        <attr name="movieArcNavigationBarDividerColor" format="color" />

        <attr name="colorControlLight" format="color" />

        <attr name="sessionListKeyline" format="dimension" />
    </declare-styleable>

    <declare-styleable name="NavigationBarContentFrameLayout">
        <attr name="navigationBarBorderColor" format="color" />
        <attr name="navigationBarBorderSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="HashtagCinemaDecoration">
        <attr name="android:drawable" />
        <attr name="margin" format="dimension" />
    </declare-styleable>

    <declare-styleable name="CircleProgressBar">
        <attr name="min" format="integer" />
        <attr name="max" format="integer" />
        <attr name="progress" format="float" />
        <attr name="progress_bar_color" format="color" />
        <attr name="progress_bar_thickness" format="dimension" />
        <attr name="text_visibility">
            <enum name="gone" value="0" />
            <enum name="visible" value="1" />
        </attr>
        <attr name="text_color" format="color" />
        <attr name="text_size" format="dimension" />
    </declare-styleable>
</resources>
